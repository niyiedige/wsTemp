				xyIds[33] = "1000850";
				xyNames[33] = "大连市分行";

				xyIds[34] = "2032349";
				xyNames[34] = "宁波市分行";

				xyIds[35] = "2044654";
				xyNames[35] = "厦门市分行";

				xyIds[36] = "2038830";
				xyNames[36] = "青岛市分行";

				xyIds[37] = "2042580";
				xyNames[37] = "深圳市分行";

				xyIds[38] = "2025996";
				xyNames[38] = "苏州分行";

				xyIds[39] = "2007107";
				xyNames[39] = "哈尔滨培训中心";

				xyIds[40] = "2046955";
				xyNames[40] = "常州培训中心";


function changeForm(value){
		var shFormObj = document.getElementById("searchJobForm");
		shFormObj.orgId.innerHTML="";
		var option = document.createElement("OPTION");
		option.value = "";
		option.text = "请选择招聘机构";
		shFormObj.orgId.options.add(option);
	if(value=='1'){
		for (var i = 0; i < xyIds.length; i++){
			option = document.createElement("OPTION");
			option.value = xyIds[i];
			option.text = xyNames[i];
			shFormObj.orgId.options.add(option);
		}
	} else if(value=='2'){
		for (var i = 0; i < shIds.length; i++){
			option = document.createElement("OPTION");
			option.value = shIds[i];
			option.text = shNames[i];
			shFormObj.orgId.options.add(option);
		}
	}else if(value=='3'){
		for (var i = 0; i < dxIds.length; i++){
			option = document.createElement("OPTION");
			option.value = dxIds[i];
			option.text = dxNames[i];
			shFormObj.orgId.options.add(option);
		}
	}else if(value=='4'){
		for (var i = 0; i < sxIds.length; i++){
			option = document.createElement("OPTION");
			option.value = sxIds[i];
			option.text = sxNames[i];
			shFormObj.orgId.options.add(option);
		}
	}
}
function submitForm(){
	var shFormObj = document.getElementById("searchJobForm");
	if(shFormObj.planType.value=="-1"){
		alert('请选择招聘类型');
	} else {
        shFormObj.submit();
	}
}
</script>
