function logReg(obj){
	var href = "";
	var tourl = location.href;
	tourl=	tourl.replace("job/index.jsp", "leaguermgr/LoginResult.jsp");
	var LOGINURL = "http://login.ccb.com/tran/WCCMainPlatV5?CCB_IBSVersion=V5&SERVLET_NAME=WCCMainPlatV5&TXCODE=L10001&tourl="+tourl+"&flag=9" ;
	var PERSONALREGISTURL = "http://login.ccb.com/tran/WCCMainB1L1?CCB_IBSVersion=V5&SERVLET_NAME=WCCMainB1L1&TXCODE=D21001&tourl="+tourl+"&flag=9";
		if("login" == obj){
			href = LOGINURL;
		}else if("register" == obj){
			href = PERSONALREGISTURL;
		}

		if("" != href) window.location.href = href;
	}
</script>
